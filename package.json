    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "bootstrap": "^4.1.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "css-loader": "0.28.7",
