{
  "name": "HApps-Store",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "hc:build": "rm -rf dna; mkdir -p dna && cd dna-src && hc package -o ../dna/happ-store.dna.json",
    "FIX-path-hc:start-holo_conductor": "mkdir -p tmp-storage && holochain -c ./home/lisa/.config/.holochain/holo/conductor-config.toml",
    "hc:start": "mkdir -p tmp-storage && holochain -c ./conductor-config.toml",
    "hc:test": "cd dna-src && hc test",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "author": "",
  "license": "GPL",
  "dependencies": {
    "@holochain/hc-redux-middleware": "^1.1.1",
    "@holochain/hc-web-client": "^0.1.3",
